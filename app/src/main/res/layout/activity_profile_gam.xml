<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.persimmon_tree_proj.Profil.profile_gam">

    <ImageButton
        android:id="@+id/go_back"
        android:layout_width="300dp"
        android:layout_height="45dp"
        android:background="#00B1A6A6"
        android:scaleType="fitCenter"
        android:src="@drawable/buttons_large_green2x"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gam8" />

    <Button
        android:id="@+id/gam"
        android:layout_width="93dp"
        android:layout_height="36dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/btn_orange_rounded"
        android:clickable="true"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="감 캐릭터"
        android:textColor="#fff"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/color"
        android:layout_width="93dp"
        android:layout_height="36dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="70dp"
        android:background="@null"
        android:clickable="true"
        android:fontFamily="@font/nanumsquare_acb"
        android:foreground="@null"
        android:text="색상"
        android:textColor="@color/greyColor"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/gam"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/gam"
        app:layout_constraintTop_toTopOf="@+id/gam" />

    <ImageView
        android:id="@+id/gam1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/btn_drawable"
        android:clickable="true"
        android:longClickable="true"
        android:onClick="Click"
        android:scaleType="fitCenter"
        android:src="@drawable/gam1"
        android:padding="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/gam2"
        app:layout_constraintEnd_toEndOf="@+id/gam"
        app:layout_constraintStart_toStartOf="@+id/gam"
        app:layout_constraintTop_toBottomOf="@+id/gam" />

    <ImageView
        android:id="@+id/gam3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:clickable="true"
        android:longClickable="true"
        android:onClick="Click"
        android:scaleType="fitCenter"
        android:src="@drawable/gam3"
        app:layout_constraintBottom_toBottomOf="@+id/gam4"
        app:layout_constraintStart_toStartOf="@+id/gam1"
        app:layout_constraintTop_toBottomOf="@+id/gam1" />

    <ImageView
        android:id="@+id/gam2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/btn_drawable"
        android:clickable="true"
        android:longClickable="true"
        android:onClick="Click"
        android:scaleType="fitCenter"
        android:src="@drawable/gam2"
        app:layout_constraintEnd_toEndOf="@+id/color"
        app:layout_constraintStart_toStartOf="@+id/color"
        app:layout_constraintTop_toBottomOf="@+id/color" />

    <ImageView
        android:id="@+id/gam5"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:clickable="true"
        android:longClickable="true"
        android:onClick="Click"
        android:scaleType="fitCenter"
        android:padding="2dp"
        android:src="@drawable/gam5"
        app:layout_constraintBottom_toBottomOf="@+id/gam6"
        app:layout_constraintStart_toStartOf="@+id/gam3"
        app:layout_constraintTop_toBottomOf="@+id/gam3" />

    <ImageView
        android:id="@+id/gam4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:clickable="true"
        android:longClickable="true"
        android:onClick="Click"
        android:scaleType="fitCenter"
        android:src="@drawable/gam4"
        app:layout_constraintEnd_toEndOf="@+id/gam2"
        app:layout_constraintStart_toStartOf="@+id/gam2"
        app:layout_constraintTop_toBottomOf="@+id/gam2" />

    <ImageView
        android:id="@+id/gam6"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:clickable="true"
        android:longClickable="true"
        android:onClick="Click"
        android:padding="2dp"
        android:scaleType="fitCenter"
        android:src="@drawable/gam6"
        app:layout_constraintEnd_toEndOf="@+id/gam4"
        app:layout_constraintStart_toStartOf="@+id/gam4"
        app:layout_constraintTop_toBottomOf="@+id/gam4" />

    <ImageView
        android:id="@+id/gam7"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:clickable="true"
        android:longClickable="true"
        android:onClick="Click"
        android:scaleType="fitCenter"
        android:src="@drawable/gam7"
        app:layout_constraintBottom_toBottomOf="@+id/gam8"
        app:layout_constraintStart_toStartOf="@+id/gam5"
        app:layout_constraintTop_toBottomOf="@+id/gam5" />

    <ImageView
        android:id="@+id/gam8"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:clickable="true"
        android:longClickable="true"
        android:onClick="Click"
        android:scaleType="fitCenter"
        android:src="@drawable/gam8"
        app:layout_constraintEnd_toEndOf="@+id/gam6"
        app:layout_constraintStart_toStartOf="@+id/gam6"
        app:layout_constraintTop_toBottomOf="@+id/gam6" />

</androidx.constraintlayout.widget.ConstraintLayout>