<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.example.persimmon_tree_proj.Mypage.profile_color">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/nanumgothicbold"
        android:text="회원 가입하기"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view2"
        android:layout_width="324dp"
        android:layout_height="1.5dp"
        android:layout_marginTop="25dp"
        android:background="#F1F0F2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />


    <ImageButton
        android:id="@+id/ok_btn"
        android:layout_width="300dp"
        android:layout_height="45dp"
        android:layout_marginBottom="48dp"
        android:background="#00B1A6A6"
        android:scaleType="fitCenter"
        android:src="@drawable/buttons_large_green2x"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.594"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/c1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="80dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#FE8189"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintEnd_toStartOf="@+id/c2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_goneMarginTop="20dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="175dp" />

    <ImageView
        android:id="@+id/c2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginEnd="80dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#FE8E69"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="175dp" />

    <ImageView
        android:id="@+id/c4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#B7DB79"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintBottom_toBottomOf="@+id/c3"
        app:layout_constraintEnd_toEndOf="@+id/c2"
        app:layout_constraintStart_toStartOf="@+id/c2"
        app:layout_constraintTop_toBottomOf="@+id/c2" />

    <ImageView
        android:id="@+id/c5"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#87dade"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintEnd_toEndOf="@+id/c3"
        app:layout_constraintStart_toStartOf="@+id/c3"
        app:layout_constraintTop_toBottomOf="@+id/c3" />

    <ImageView
        android:id="@+id/c7"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#A1AEE5"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintEnd_toEndOf="@+id/c5"
        app:layout_constraintStart_toStartOf="@+id/c5"
        app:layout_constraintTop_toBottomOf="@+id/c5" />

    <ImageView
        android:id="@+id/c6"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#99CAEB"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintBottom_toBottomOf="@+id/c5"
        app:layout_constraintEnd_toEndOf="@+id/c4"
        app:layout_constraintStart_toStartOf="@+id/c4"
        app:layout_constraintTop_toBottomOf="@+id/c4" />

    <ImageView
        android:id="@+id/c8"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#E89CDA"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintBottom_toBottomOf="@+id/c7"
        app:layout_constraintEnd_toEndOf="@+id/c6"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="@+id/c6"
        app:layout_constraintTop_toBottomOf="@+id/c6" />

    <ImageView
        android:id="@+id/c3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#FEC56C"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintEnd_toEndOf="@+id/c1"
        app:layout_constraintStart_toStartOf="@+id/c1"
        app:layout_constraintTop_toBottomOf="@+id/c1"
        app:layout_goneMarginTop="20dp" />


</androidx.constraintlayout.widget.ConstraintLayout>