<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.example.persimmon_tree_proj.Mypage.profile_color">

    <ImageButton
        android:id="@+id/go_back"
        android:layout_width="300dp"
        android:layout_height="45dp"
        android:background="#00B1A6A6"
        android:scaleType="fitCenter"
        android:src="@drawable/buttons_large_green2x"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/c7" />


    <ImageView
        android:id="@+id/c1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#FE8189"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_goneMarginTop="20dp"
        tools:layout_editor_absoluteX="88dp" />

    <ImageView
        android:id="@+id/c2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#FE8E69"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintBottom_toBottomOf="@+id/c1"
        tools:layout_editor_absoluteX="250dp" />

    <ImageView
        android:id="@+id/c4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#B7DB79"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintBottom_toBottomOf="@+id/c3"
        app:layout_constraintEnd_toEndOf="@+id/c2"
        app:layout_constraintStart_toStartOf="@+id/c2"
        app:layout_constraintTop_toBottomOf="@+id/c2" />

    <ImageView
        android:id="@+id/c5"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#87dade"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintEnd_toEndOf="@+id/c3"
        app:layout_constraintStart_toStartOf="@+id/c3"
        app:layout_constraintTop_toBottomOf="@+id/c3" />

    <ImageView
        android:id="@+id/c7"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#A1AEE5"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintEnd_toEndOf="@+id/c5"
        app:layout_constraintStart_toStartOf="@+id/c5"
        app:layout_constraintTop_toBottomOf="@+id/c5" />

    <ImageView
        android:id="@+id/c6"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#99CAEB"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintBottom_toBottomOf="@+id/c5"
        app:layout_constraintEnd_toEndOf="@+id/c4"
        app:layout_constraintStart_toStartOf="@+id/c4"
        app:layout_constraintTop_toBottomOf="@+id/c4" />

    <ImageView
        android:id="@+id/c8"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#E89CDA"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintBottom_toBottomOf="@+id/c7"
        app:layout_constraintEnd_toEndOf="@+id/c6"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="@+id/c6"
        app:layout_constraintTop_toBottomOf="@+id/c6" />

    <ImageView
        android:id="@+id/c3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_drawable"
        android:backgroundTint="#FEC56C"
        android:clickable="true"
        android:onClick="Click"
        app:layout_constraintEnd_toEndOf="@+id/c1"
        app:layout_constraintStart_toStartOf="@+id/c1"
        app:layout_constraintTop_toBottomOf="@+id/c1"
        app:layout_goneMarginTop="20dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:text="프로필 설정하기"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>