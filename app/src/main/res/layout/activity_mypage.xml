<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="com.example.persimmon_tree_proj.MypageActivity">

    <ImageButton
        android:id="@+id/go_back"
        android:layout_width="20dp"
        android:layout_height="16dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:background="#00B1A6A6"
        android:scaleType="fitCenter"
        android:src="@drawable/go_backk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />


    <ImageButton
        android:id="@+id/edit_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="69dp"
        android:layout_marginRight="130dp"
        android:background="#fff"
        android:src="@drawable/mypagefix_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/profile_text"
        android:layout_width="142dp"
        android:layout_height="27dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="48dp"
        android:height="30dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="프로필"
        android:textColor="@color/mainColor"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <ImageView
        android:id="@+id/profile_image"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="71dp"
        android:foreground="@drawable/btn_grey_rounded"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.532"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/my_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_acdb"
        android:text="나의 아이디는?"
        android:textColor="#817889"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/text_id"
        app:layout_constraintTop_toBottomOf="@+id/text_id" />

    <TextView
        android:id="@+id/text_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="아이디"
        android:textColor="#000"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/my_introduce"
        app:layout_constraintTop_toBottomOf="@+id/my_introduce" />

    <TextView
        android:id="@+id/my_fcode"
        android:layout_width="99dp"
        android:layout_height="20dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/nanumsquare_acdb"
        android:text="우리 가족 코드"
        android:textColor="#817889"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/text_family_code"
        app:layout_constraintTop_toBottomOf="@+id/text_family_code"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/text_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="200dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:padding="10dp"
        android:text="한 줄 소개"
        android:textColor="#000"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/my_introduce"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/my_introduce"
        android:layout_width="300dp"
        android:layout_height="35dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/btn_grey_rounded"
        android:padding="8dp"
        android:text="나의 별명은?"
        android:textColor="#000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_nickname"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/text_membernum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="가족 구성원"
        android:textColor="#000"
        android:textSize="14dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="@+id/my_id"
        app:layout_constraintTop_toBottomOf="@+id/my_id" />

    <TextView
        android:id="@+id/my_membernum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="구성원 수는"
        android:textColor="#817889"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/text_membernum"
        app:layout_constraintTop_toBottomOf="@+id/text_membernum"
        />

    <TextView
        android:id="@+id/text_family_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="우리 가족 코드"
        android:textColor="#000"
        android:textSize="14dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="@+id/my_membernum"
        app:layout_constraintTop_toBottomOf="@+id/my_membernum"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <Button
        android:id="@+id/btn_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="56dp"
        android:background="#00FFFFFF"
        android:text="로그아웃"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/mun2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="문의 : 013-111-0912"
        app:layout_constraintBottom_toTopOf="@+id/gamnamoo_email"
        app:layout_constraintStart_toStartOf="@+id/gamnamoo_email"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/gamnamoo_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="gammnamooo@gmail.com"
        app:layout_constraintBottom_toTopOf="@+id/btn_logout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />


</androidx.constraintlayout.widget.ConstraintLayout>