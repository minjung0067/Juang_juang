<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="com.example.persimmon_tree_proj.Mypage.MypageActivity">


    <ImageButton
        android:id="@+id/go_back"
        android:layout_width="20dp"
        android:layout_height="16dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="16dp"
        android:background="#00B1A6A6"
        android:scaleType="fitCenter"
        android:src="@drawable/go_backk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />


    <ImageButton
        android:id="@+id/edit_btn"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginLeft="80dp"
        android:layout_marginBottom="80dp"
        android:background="#fff"
        android:scaleType="fitCenter"
        android:src="@drawable/mypagefix_icon2x"
        app:layout_constraintBottom_toBottomOf="@+id/profile_image"
        app:layout_constraintStart_toStartOf="@+id/profile_image" />

    <TextView
        android:id="@+id/profile_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="60dp"
        android:height="30dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="프로필"
        android:textColor="@color/mainColor"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

<!--    <ImageView-->
<!--        android:id="@+id/profile_image_strok"-->
<!--        android:layout_width="93dp"-->
<!--        android:layout_height="93dp"-->
<!--        android:background="@drawable/btn_grey_rounded"-->
<!--        app:layout_constraintBottom_toBottomOf="@+id/profile_image"-->
<!--        app:layout_constraintEnd_toEndOf="@+id/profile_image"-->
<!--        app:layout_constraintStart_toStartOf="@+id/profile_image"-->
<!--        app:layout_constraintTop_toTopOf="@+id/profile_image" />-->

    <ImageView
        android:id="@+id/profile_image"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/profile_outline"
        android:src="@drawable/gam1"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.563"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_text"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/my_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="나의 아이디는?"
        android:textColor="#817889"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/text_id"
        app:layout_constraintTop_toBottomOf="@+id/text_id" />

    <TextView
        android:id="@+id/text_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="아이디"
        android:textColor="#000"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/my_introduce"
        app:layout_constraintTop_toBottomOf="@+id/my_introduce" />

    <TextView
        android:id="@+id/my_fcode"
        android:layout_width="99dp"
        android:layout_height="20dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="우리 가족 코드"
        android:textColor="#817889"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/text_family_code"
        app:layout_constraintTop_toBottomOf="@+id/text_family_code"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/text_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="한 줄 소개"
        android:textColor="#000"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/my_introduce"
        app:layout_constraintTop_toBottomOf="@+id/profile_image" />

    <TextView
        android:id="@+id/my_introduce"
        android:layout_width="300dp"
        android:layout_height="35dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/btn_grey_rounded"
        android:padding="10dp"
        android:text="나의 별명은?"
        android:textColor="#000"
        android:fontFamily="@font/nanumsquare_acb"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_nickname"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/text_membernum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="가족 구성원"
        android:textColor="#000"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/my_id"
        app:layout_constraintTop_toBottomOf="@+id/my_id" />

    <TextView
        android:id="@+id/my_membernum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="구성원 수는"
        android:textColor="#817889"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/text_membernum"
        app:layout_constraintTop_toBottomOf="@+id/text_membernum" />

    <TextView
        android:id="@+id/text_family_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="우리 가족 코드"
        android:textColor="#000"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/my_membernum"
        app:layout_constraintTop_toBottomOf="@+id/my_membernum"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <Button
        android:id="@+id/btn_logout"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:background="@drawable/btn_white_rounded"
        android:text="로그아웃"
        android:textSize="13dp"
        app:layout_constraintBottom_toTopOf="@+id/mun2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/my_fcode"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/mun2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/nanumsquare_acr"
        android:text="문의 : instagram @juang_juang5"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/gamnamoo_email"
        app:layout_constraintStart_toStartOf="@+id/gamnamoo_email"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/gamnamoo_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_acr"
        android:text="gammnamooo@gmail.com"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:layout_marginBottom="16dp"
        android:background="@color/brightgray"
        android:gravity="center_horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="-28dp" />

    <ImageButton
        android:id="@+id/calendar_btn"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginBottom="20dp"
        android:background="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/calendar_icon2x"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/main_btn"
        app:layout_constraintStart_toEndOf="@+id/qna_btn" />

    <ImageButton
        android:id="@+id/main_btn"
        android:layout_width="70dp"
        android:layout_height="62dp"
        android:layout_marginBottom="12dp"
        android:background="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/check_icon_big"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/to_do_btn"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="20dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:src="@drawable/clinet_sound_icon2x"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/game_btn"
        app:layout_constraintStart_toEndOf="@+id/main_btn" />

    <ImageButton
        android:id="@+id/game_btn"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="20dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:src="@drawable/clinet_sound_icon2x"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView2" />

    <ImageButton
        android:id="@+id/qna_btn"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="20dp"
        android:background="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/write_icon2x"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>